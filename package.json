    "scripts": {
        "tsn": "ts-node --transpile-only -r tsconfig-paths/register -r dotenv/config",
        "tsnd": "ts-node-dev --transpileOnly --respawn --no-notify --no-deps -r tsconfig-paths/register -r source-map-support/register -r dotenv/config",
        "dev-admin": "yarn tsnd adminSite/server/app.tsx",
        "tsnd:inspect": "ts-node-dev --inspect --transpileOnly --respawn --no-notify --no-deps -r tsconfig-paths/register -r source-map-support/register -r dotenv/config",
        "dev-admin:inspect": "yarn tsnd:inspect adminSite/server/app.tsx",
        "dev-webpack": "webpack-dev-server -d",
        "dev": "npm-run-all --race --parallel dev-admin dev-webpack",
        "dev-tmux": "tmex dev \"yarn tsn adminSite/server/app.tsx\" \"yarn dev-webpack\"",
        "typeorm": "yarn tsn node_modules/.bin/typeorm",
        "knex": "yarn tsn node_modules/.bin/knex --knexfile knexfile.ts",
        "migrate": "yarn knex migrate:latest && yarn typeorm migration:run",
        "lint": "eslint .",
        "fix": "eslint . --fix",
        "prettify": "yarn pretty-quick --pattern \"**/*.{tsx,ts,jsx,js,json,md,html,css,scss,yml}\"",
        "prettify:check": "yarn prettify --check",
        "prettify-all": "yarn prettier --write \"**/*.{tsx,ts,jsx,js,json,md,html,css,scss,yml}\"",
        "prettify-all:check": "yarn prettier --check \"**/*.{tsx,ts,jsx,js,json,md,html,css,scss,yml}\"",
        "typecheck": "tsc --noEmit",
        "build": "rm -rf dist && ENV=production webpack -p --progress",
        "test": "jest",
        "test-watch": "jest --watchAll",
        "test-single-process": "jest --maxWorkers=1",
        "coverage": "jest --coverage=true --coverageProvider=v8 && open coverage/lcov-report/index.html",
        "test-leaks": "yarn test --detectLeaks",
        "cypress:open": "yarn cypress open --project .",
        "testcheck": "./deploy/testcheck.sh",
        "deploy": "./deploy/deploy.sh",
        "serve": "yarn tsn adminSite/server/app.tsx",
        "bake": "yarn tsn deploy/bakeSite.ts",
        "post-update": "yarn tsn db/postUpdatedHook.ts",
        "deploy-queue": "yarn tsn deploy/watch.ts",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook -o .storybook/build",
        "bundlewatch": "ENV=production webpack -p && bundlewatch --config .bundlewatch.config.json",
        "bundlewatch-json": "ENV=production webpack -p --json > dist/webpack-bundle-stats.json && bundlewatch --config .bundlewatch.config.json",
        "live-commit": "yarn tsn deploy/liveCommit.ts"
    },
